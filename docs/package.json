    "@fa-repo/shard-docs": "^5.0.0-2",
    "@mdx-js/loader": "^1.5.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "laravel-mix": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "source-map-explorer": "^2.0.1",
    "vue-template-compiler": "^2.7.8"
  }
